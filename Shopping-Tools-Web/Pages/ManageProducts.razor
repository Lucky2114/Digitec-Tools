@page "/ManageProducts"
@using Shopping_Tools.Data.Enums
@using Shopping_Tools_Api_Services.Core.Brack
@using Shopping_Tools_Api_Services.Core.Digitec

<AuthorizeView>
    <Authorized>
        <ImplementedShopsComponent ImplementedShops="@_implementedShops" />
    </Authorized>

    <NotAuthorized>
        <NotAuthorizedComponent/>
    </NotAuthorized>
</AuthorizeView>

@code {
    private readonly List<IApi> _implementedShops = new List<IApi>()
    {
        new Digitec(),
        new Brack()
    };
}